    "def __get_reviews(url):\n",
    "    headers = {\n",
    "        'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/58.0.3029.110 Safari/537.3',\n",
    "        'Accept-Language': 'en-US,en;q=0.5',\n",
    "        'Accept-Encoding': 'gzip, deflate, br',\n",
    "        'Connection': 'keep-alive',\n",
    "        'Upgrade-Insecure-Requests': '1',\n",
    "        'DNT': '1'\n",
    "    }\n",
    "    \n",
    "    # Tentar até 5 vezes com um atraso de 5 segundos entre as tentativas\n",
    "    for _ in range(5):\n",
    "        response = requests.get(url, headers=headers)\n",
    "        if response.status_code == 200:\n",
    "            break # volta pro for p nao dar erro\n",
    "        else:\n",
    "            print(f\"Failed to retrieve the web page. Status code: {response.status_code}. Retrying...\")\n",
    "            time.sleep(5)# NAO tentar tai rapido\n",
    "    else:\n",
    "        # Se depois de 5 tentativas ainda falhar, retornar um erro\n",
    "        print(\"Falha ao recuperar a página da web após várias tentativas nao deu meu parcairo .\")\n",
    "        return [] # se o for acabar exiba ^\n",
    "\n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    reviews = [] #padrao ne criar lista p ir colocar as avaliacoes\n",
    "\n",
    "    for review in soup.find_all(\"span\", {\"data-hook\": \"review-body\"}): # passa pelo site ate acabar as avalicoes assim nao fica engessado\n",
    "        reviews.append(review.get_text().strip()) # vai add na lista \n",
    "\n",
    "    return reviews # mostra a lista \n",
    "\n",
    "# Exemplo de uso\n",
    "\n",
